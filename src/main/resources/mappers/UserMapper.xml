<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mintae.dating.mapper.UserMapper">

    <select id="findByMobile" resultType="com.mintae.dating.vo.User">
        SELECT * FROM user WHERE mobile = #{mobile}
    </select>

    <insert id="signup">
        insert into user(mobile, role) values(#{mobile},#{role})
    </insert>


</mapper>